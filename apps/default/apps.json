nstallButton);appElement.dataset.name=app.name;appElement.dataset.url=app.url;appElement.dataset.icon=app.icon;appContainer.appendChild(appElement)});}function filterApps(){const searchQuery=document.getElementById('search-bar').value.toLowerCase();filteredApps=apps.filter(app=>app.name.toLowerCase().includes(searchQuery));displayApps(filteredApps);}function installApp(name,icon,url){const appElement=document.querySelector(`[data-name='${name}']`);const installButton=appElement.querySelector('.install-btn');const uninstallButton=appElement.querySelector('.uninstall-btn');installButton.style.display='none';uninstallButton.style.display='inline-block';console.log(`Installing ${name}...`);}function uninstallApp(name){const appElement=document.querySelector(`[data-name='${name}']`);const installButton=appElement.querySelector('.install-btn');const uninstallButton=appElement.querySelector('.uninstall-btn');installButton.style.display='inline-block';uninstallButton.style.display='none';console.log(`Uninstalling ${name}...`);}displayApps(filteredApps);</script></body></html>"
